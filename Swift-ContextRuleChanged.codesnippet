	<key>IDECodeSnippetSummary</key>
	<string>Register for RuleChanged Notifications</string>
	<key>IDECodeSnippetTitle</key>
	<string>Context Rule Changed Notification</string>
	<key>IDECodeSnippetUserSnippet</key>
	<true/>
	<key>IDECodeSnippetVersion</key>
	<integer>0</integer>
</dict>
