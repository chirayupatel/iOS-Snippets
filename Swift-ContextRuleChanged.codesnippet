	<key>IDECodeSnippetContents</key>
	<string>// Register for RuleChanged notifications
        let token = NSNotificationCenter.defaultCenter().addObserverForName(ContextManager.Constants.ContextRuleChanged, object: &lt;#T##AnyObject?#&gt;, queue: &lt;#T##NSOperationQueue?#&gt;) { (notification) -&gt; Void in
            // Retrieve rule from userInfo
            if let rule = (notification.userInfo as? [String : FlybitsSDK.Rule])?[ContextManager.Constants.ContextRule], ruleName = rule.name {
                &lt;#code#&gt;
            }
        }</string>
